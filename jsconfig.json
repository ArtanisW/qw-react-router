{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@src/*": [
        "./src/*"
      ],
      "@common/*": [
        "./src/common/*"
      ],
      "@components/*": [
        "./src/components/*"
      ],
      "@constants/*": [
        "./src/constants/*"
      ],
    }
  },
  "includes": [
    "src/**/*"
  ]
}